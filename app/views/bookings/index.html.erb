<% form_for :booking, :url => bookings_path, :html => {:id => 'claim-form'} do |form| %>
  <%= form.label :phone, 'Enter phone no:', :style => 'width: 180px; text-align: right' %>
  <%= form.text_field :phone, :class => 'phone-box' %>
  <%= form.submit 'Search' %>
<% end %>
<div class="clear"></div>
<small style="display: block; padding-left: 190px; font-size: 12px; color: #999">Enter phone number without any symbols, or dashes, i.e. <strong>0123456789</strong></small>